<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="#F7FAFC"
    android:layout_height="match_parent">
    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/home_title_layout"
        ></include>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    <LinearLayout
        android:layout_margin="2dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout

            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="179dp">
            <FrameLayout

                android:id="@+id/table_content"
                android:layout_weight="1"
                android:layout_width="0dp"

                android:layout_marginLeft="2dp"
                android:layout_marginTop="2dp"
                android:layout_height="match_parent">

            </FrameLayout>

            <FrameLayout

                android:layout_marginRight="10dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="7dp"
                android:id="@+id/notice_content"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">

            </FrameLayout>

        </LinearLayout>

        <LinearLayout
           android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="3dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="177dp">
            <FrameLayout
                android:id="@+id/kq_content"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">

            </FrameLayout>


        </LinearLayout>
        <LinearLayout
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="177dp">
            <FrameLayout
                android:id="@+id/banner_content"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">

            </FrameLayout>


        </LinearLayout>
        <LinearLayout
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="177dp">
            <FrameLayout
                android:id="@+id/work_content"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">

            </FrameLayout>


        </LinearLayout>

        <LinearLayout
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="177dp">
            <FrameLayout
                android:id="@+id/class_honor_content"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">

            </FrameLayout>


        </LinearLayout>

        <LinearLayout
           android:padding="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="293dp">
            <FrameLayout
                android:id="@+id/student_honor_content"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">

            </FrameLayout>


        </LinearLayout>
    </LinearLayout>


    </ScrollView>
</LinearLayout>
